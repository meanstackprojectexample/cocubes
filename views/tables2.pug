html
  head
    meta(charset='utf-8')
    meta(http-equiv='X-UA-Compatible', content='IE=edge')
    meta(name='viewport', content='width=device-width, initial-scale=1, shrink-to-fit=no')
    meta(name='description', content='')
    meta(name='author', content='')
    script(src='https://ajax.googleapis.com/ajax/libs/jquery/3.4.0/jquery.min.js')
    link(href='https://cdn.datatables.net/1.10.19/css/jquery.dataTables.min.css', rel='stylesheet')
    link(href='https://cdn.datatables.net/buttons/1.5.6/css/buttons.dataTables.min.css', rel='stylesheet')
    title Home Page
    // Custom fonts for this template
    link(href='javascripts/vendor/fontawesome-free/css/all.min.css', rel='stylesheet', type='text/css')
    link(href='https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i', rel='stylesheet')
    // Custom styles for this template
    link(href='stylesheets/css/sb-admin-2.min.css', rel='stylesheet')
  body
    // Page Wrapper
    #wrapper
      // Sidebar
      ul#accordionSidebar.navbar-nav.bg-gradient-primary.sidebar.sidebar-dark.accordion
        // Sidebar - Brand
        a.sidebar-brand.d-flex.align-items-center.justify-content-center(href='')
          .sidebar-brand-icon.rotate-n-15
            i.fa.fa-graduation-cap
          .sidebar-brand-text.mx-3 COCUBES 
        // Divider
        hr.sidebar-divider.my-0
        // Nav Item - Dashboard
        li.nav-item
          a.nav-link(href='/admin2')
            i.fas.fa-home
            span Home
        // Divider
        hr.sidebar-divider
        // Heading
        .sidebar-heading
          | Interface
        // Nav Item - Pages Collapse Menu
        li.nav-item
          a.nav-link.collapsed(href='#', data-toggle='collapse', data-target='#collapseTwo', aria-expanded='true', aria-controls='collapseTwo')
            i.fas.fa-fw.fa-cog
            span  Statistics
          #collapseTwo.collapse(aria-labelledby='headingTwo', data-parent='#accordionSidebar')
            .bg-white.py-2.collapse-inner.rounded
              h6.collapse-header Overall comparision
              a.collapse-item(href='/college') Colleges
              a.collapse-item(href='/branch') Branches
              a.collapse-item(href='/year') Years
        // Nav Item - Utilities Collapse Menu
        // Nav Item - Pages Collapse Menu
        // Divider
        hr.sidebar-divider
        // Heading
        // Nav Item - Pages Collapse Menu
        // Nav Item - Charts
        li.nav-item
          a.nav-link(href='/charts2')
            i.fas.fa-fw.fa-chart-area
            span Charts
        // Nav Item - Tables
        li.nav-item
          a.nav-link(href='/tables2')
            i.fas.fa-fw.fa-table
            span Tables
        li.nav-item
          a.nav-link(href='/role2')
            i.fas.fa-fw.fa-table
            span Role    
        // Divider
        hr.sidebar-divider.d-none.d-md-block
        // Sidebar Toggler (Sidebar)
        .text-center.d-none.d-md-inline
          button#sidebarToggle.rounded-circle.border-0
      // End of Sidebar
      // Content Wrapper
      #content-wrapper.d-flex.flex-column
        // Main Content
        #content
          // Topbar
          nav.navbar.navbar-expand.navbar-light.bg-white.topbar.mb-4.static-top.shadow
            
            img(style="width:120px" src='images/logo.png') 
            
            
            
           
            ul.navbar-nav.ml-auto
              // Nav Item - Messages
              .topbar-divider.d-none.d-sm-block
    
              // Nav Item - User Information
              li.nav-item.dropdown.no-arrow
                a#userDropdown.nav-link.dropdown-toggle(href='#', role='button', data-toggle='dropdown', aria-haspopup='true', aria-expanded='false')
                  span.mr-2.d-none.d-lg-inline.text-gray-600.small
                  p #{user.NAME}
                  img.img-profile.rounded-circle(src="images/"+user.ID+".JPG")
                // Dropdown - User Information
                .dropdown-menu.dropdown-menu-right.shadow.animated--grow-in(aria-labelledby='userDropdown')
                  a.dropdown-item(href='#')
                    i.fas.fa-user.fa-sm.fa-fw.mr-2.text-gray-400
                    |   Profile
                  a.dropdown-item(href='#')
                    i.fas.fa-cogs.fa-sm.fa-fw.mr-2.text-gray-400
                    |     Settings
                  a.dropdown-item(href='/changepassword')
                    i.fas.fa-list.fa-sm.fa-fw.mr-2.text-gray-400
                    |       Change Password
                  .dropdown-divider
                  a.dropdown-item(href='/logout')
                    i.fas.fa-sign-out-alt.fa-sm.fa-fw.mr-2.text-gray-400
                    |       Logout
          // End of Topbar
          // Begin Page Content
          .container-fluid
            // Page Heading
            
            p.mb-4
              |For more information about Students Performance.
            // DataTales Example
            .card.shadow.mb-4
              .card-header.py-3
                h6.m-0.font-weight-bold.text-primary DataTables Example
              .card-body
                .table-responsive
                  table.table.table-bordered.example(width='100%', cellspacing='0')
                    thead
                      tr
                        th Name
                        th Rollno
                        th Branch
                        th College
                        th Aptitude
                        th Reasoning
                        th English
                        th Coding
                        th wet
                        th Score
                    tbody
                      each member in data
                        tr
                          td #{member.StudentName}
                          td #{member.RollNumber}
                          td #{member.Branch}
                          td #{member.College}
                          td #{member.Aptitude}
                          td #{member.Quantitative}
                          td #{member.English}
                          td #{member.Coding}
                          td #{member.WrittenEnglish}
                          td #{member.Score}
          // container-fluid
          // End of Main Content
          // End of Content Wrapper
          // End of Page Wrapper
          // Scroll to Top Button
          a.scroll-to-top.rounded(href='#page-top')
            i.fas.fa-angle-up
          // Logout Modal
          #logoutModal.modal.fade(tabindex='-1', role='dialog', aria-labelledby='exampleModalLabel', aria-hidden='true')
            .modal-dialog(role='document')
              .modal-content
                .modal-header
                  h5#exampleModalLabel.modal-title Ready to Leave?
                  button.close(type='button', data-dismiss='modal', aria-label='Close')
                    span(aria-hidden='true') Ã—
                .modal-body Select "Logout" below if you are ready to end your current session.
                .modal-footer
                  button.btn.btn-secondary(type='button', data-dismiss='modal') Cancel
                  a.btn.btn-primary(href='login.html') Logout
          // Bootstrap core JavaScript
          script(src='javascripts/vendor/bootstrap/js/bootstrap.bundle.min.js')
          // Core plugin JavaScript
          script(src='javascripts/vendor/jquery-easing/jquery.easing.min.js')
          // Custom scripts for all pages
          // Page level plugins
          | //-
          script(src='javascripts/vendor/datatables/jquery.dataTables.min.js')
          script(src='javascripts/vendor/datatables/dataTables.bootstrap4.min.js')
          // Page level custom scripts
          script(src='javascripts/tables.js')
          script(src='https://code.jquery.com/jquery-3.3.1.js')
          script(src='https://cdn.datatables.net/1.10.19/js/jquery.dataTables.min.js')
          script(src='https://cdn.datatables.net/buttons/1.5.6/js/dataTables.buttons.min.js')
          script(src='https://cdn.datatables.net/buttons/1.5.6/js/buttons.flash.min.js')
          script(src='https://cdnjs.cloudflare.com/ajax/libs/jszip/3.1.3/jszip.min.js')
          script(src='https://cdnjs.cloudflare.com/ajax/libs/pdfmake/0.1.53/pdfmake.min.js')
          script(src='https://cdnjs.cloudflare.com/ajax/libs/pdfmake/0.1.53/vfs_fonts.js')
          script(src='https://cdn.datatables.net/buttons/1.5.6/js/buttons.html5.min.js')
          script(src='https://cdn.datatables.net/buttons/1.5.6/js/buttons.print.min.js')
