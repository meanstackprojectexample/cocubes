  body#page-top
    // Page Wrapper
    #wrapper
      // Sidebar
      ul#accordionSidebar.navbar-nav.bg-gradient-primary.sidebar.sidebar-dark.accordion
        // Sidebar - Brand
        a.sidebar-brand.d-flex.align-items-center.justify-content-center(href='')
          .sidebar-brand-icon.rotate-n-15
            i.fa.fa-graduation-cap
          .sidebar-brand-text.mx-3 COCUBES 
        // Divider
        hr.sidebar-divider.my-0
        // Nav Item - Dashboard
        li.nav-item
          a.nav-link(href='/admin')
            i.fas.fa-home
            span Home
        // Divider
        hr.sidebar-divider
        // Heading
        .sidebar-heading
          | Interface
        // Nav Item - Pages Collapse Menu
        li.nav-item
          a.nav-link.collapsed(href='#', data-toggle='collapse', data-target='#collapseTwo', aria-expanded='true', aria-controls='collapseTwo')
            i.fas.fa-fw.fa-cog
            span  Select Years
          #collapseTwo.collapse(aria-labelledby='headingTwo', data-parent='#accordionSidebar')
            .bg-white.py-2.collapse-inner.rounded
              h6.collapse-header Year Wise
              a.collapse-item(href='/finalyear') IV Year
              a.collapse-item(href='/thirdyear') III YEAR
              a.collapse-item(href='/secondyear') II YEAR
        // Nav Item - Utilities Collapse Menu
        // Divider
        hr.sidebar-divider
        // Heading
        // Nav Item - Pages Collapse Menu
        // Nav Item - Charts
        li.nav-item
          a.nav-link(href='/statistics')
            i.fas.fa-fw.fa-chart-area
            span statistics
        // Nav Item - Tables
        li.nav-item
          a.nav-link(href='/tables')
            i.fas.fa-fw.fa-table
            span Tables
        li.nav-item
          a.nav-link(href='/role')
            i.fas.fa-fw.fa-table
            span Role
        // Divider
        hr.sidebar-divider.d-none.d-md-block
        // Sidebar Toggler (Sidebar)
        .text-center.d-none.d-md-inline
          button#sidebarToggle.rounded-circle.border-0
      // End of Sidebar
      // Content Wrapper
      #content-wrapper.d-flex.flex-column
        // Main Content
        #content
          // Topbar
          nav.navbar.navbar-expand.navbar-light.bg-white.topbar.mb-4.static-top.shadow
            img(style="width:120px" src='images/logo.png') 
            
            
            ul.navbar-nav.ml-auto
                .topbar-divider.d-none.d-sm-block
              
              
            li.nav-item.dropdown.no-arrow
                a#userDropdown.nav-link.dropdown-toggle(href='#', role='button', data-toggle='dropdown', aria-haspopup='true', aria-expanded='false')
                  span.mr-2.d-none.d-lg-inline.text-gray-600.small
                  p #{user.NAME}
                  img.img-profile.rounded-circle(src="images/"+user.ID+".PNG")
                // Dropdown - User Information
                .dropdown-menu.dropdown-menu-right.shadow.animated--grow-in(aria-labelledby='userDropdown')
                  a.dropdown-item(href='#')
                    i.fas.fa-user.fa-sm.fa-fw.mr-2.text-gray-400
                    |   Profile
                  a.dropdown-item(href='#')
                    i.fas.fa-cogs.fa-sm.fa-fw.mr-2.text-gray-400
                    |     Settings
                  a.dropdown-item(href='/changepassword')
                    i.fas.fa-list.fa-sm.fa-fw.mr-2.text-gray-400
                    |       Change Password
                  .dropdown-divider
                  a.dropdown-item(href='/logout')
                    i.fas.fa-sign-out-alt.fa-sm.fa-fw.mr-2.text-gray-400
                    |       Logout
  
    
       
        .container-fluid
        
        
         p.mb-4
          |  For more information about Students Performance. 
          
        
         .card.shadow.mb-4
          .card-header.py-3
            h6.m-0.font-weight-bold.text-primary Student Details
          .card-body
            .table-responsive
              table.table.table-bordered.example(width='100%', cellspacing='0')
                thead
                  tr
                    
                    th Name
                    th Rollno
                    th Branch
                    th College
                    th Aptitude
                    th Reasoning
                    th English
                    th Coding
                    th wet
                    th Score
                    th Semister
                    th view
                    
                tbody
                  each member in data
                    tr
                      
                      td #{member.StudentName}
                      td #{member.RollNumber}
                      td #{member.Branch}
                      td #{member.College}
                      td #{member.Aptitude}
                      td #{member.Quantitative}
                      td #{member.English}
                      td #{member.Coding}
                      td #{member.WrittenEnglish}
                      td #{member.Score}
                      td #{member.Sem}
                      td
                        button.btn.btn-primary(type='button', data-toggle='modal', data-target='#myModal', value=member._id).view VIEW
      #myModal.modal.fade
        .modal-dialog.modal-xl
          .modal-content
            .modal-header
              h3.modal-title Student details
              button.close(type='button', style='color: blue;', data-dismiss='modal') Ã—
            // Modal Header
            .container
              .resume
                .row
                  .col-xs-12.col-sm-12.col-md-12.col-lg-12
                    .panel.panel-default
                      .panel-heading.resume-heading
                        .row
                          .col-xs-12.col-sm-4
                            figure
                              img.img-circle.img-responsive(style='width:200px;', alt='', src='images/sai.jpg')
                          .col-xs-12.col-sm-6
                            ul.list-group
                              li.list-group-item#studentroll
                              li.list-group-item#studentname
                              li.list-group-item#studentcollege
                              li.list-group-item#Sem
                  .bs-callout.bs-callout-danger.col-sm-12.w3-card-4
                    h4 SecondYear Sem1
                    p Aptitude
                    .w3-light-grey.w3-round-xlarge.w3-small
                      .w3-container.w3-center.w3-round-xlarge.w3-blue.AptitudePercent
                        .w3-center.w3-text-white.AptitudeText.
                        
                    p Reasoning
                    .w3-light-grey.w3-round-xlarge.w3-small
                      .w3-container.w3-center.w3-round-xlarge.w3-blue.AnalyticalPercent
                        .w3-center.w3-text-white.AnalyticalText
                    p English
                    .w3-light-grey.w3-round-xlarge.w3-small
                      .w3-container.w3-center.w3-round-xlarge.w3-blue.EnglishPercent
                        .w3-center.w3-text-white.EnglishText
                    p Coding
                    .w3-light-grey.w3-round-xlarge.w3-small
                      .w3-container.w3-center.w3-round-xlarge.w3-blue.CodingPercent
                        .w3-center.w3-text-white.CodingText
                .card-body
                  .table-responsive
                      table.table.table-bordered.example1(width='100%', cellspacing='0')
                        thead
                          tr
                            th CorePlant
                            th CoreRD
                            th ITESBPO
                            th ITProduct
                            th ITServices
                            th KPO
                            th NetworkAdmin
                            th Operation
                            th Sales
                            th Software
                        tbody
                          tr
                            td#studentcore
                            td#studentcoreRd
                            td#studentites
                            td#studentitproduct
                            td#studentitservices
                            td#studentkpo
                            td#studentnetwork
                            td#studentoperations
                            td#studentsales
                            td#studentsoftware


        
                  
            // Modal footer
            .modal-footer
              button.btn.btn-primary(type='button', data-dismiss='modal') Close

                        
      // End of Main Content
      // End of Content Wrapper
// End of Page Wrapper
